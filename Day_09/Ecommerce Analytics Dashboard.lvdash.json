{
  "datasets": [
    {
      "name": "412fb008",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `workspace`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "-- Revenue trend (daily)\n",
        "SELECT date(event_time) AS date, SUM(price) AS total_revenue \n",
        "FROM delta.`dbfs:/Volumes/workspace/ecommerce/silver/events_delta` \n",
        "GROUP BY date(event_time) ORDER BY date;"
      ]
    },
    {
      "name": "ee1d2dce",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `workspace`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "-- Top products\n",
        "SELECT product_id, SUM(price) AS total_sales \n",
        "FROM delta.`dbfs:/Volumes/workspace/ecommerce/silver/events_delta` \n",
        "GROUP BY product_id ORDER BY total_sales DESC LIMIT 10;"
      ]
    },
    {
      "name": "dc462c70",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `workspace`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "-- Conversion funnel (view → cart → purchase example)\n",
        "SELECT event_type, COUNT(*) AS event_count \n",
        "FROM delta.`dbfs:/Volumes/workspace/ecommerce/silver/events_delta` \n",
        "GROUP BY event_type ORDER BY event_count DESC;"
      ]
    },
    {
      "name": "8a22c6c3",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `workspace`;\n",
        "USE SCHEMA `default`;\n",
        "\n",
        "-- Top products\n",
        "SELECT brand, SUM(price) AS total_sales \n",
        "FROM delta.`dbfs:/Volumes/workspace/ecommerce/silver/events_delta` \n",
        "WHERE brand IS NOT NULL\n",
        "GROUP BY brand ORDER BY total_sales DESC LIMIT 10;"
      ]
    }
  ],
